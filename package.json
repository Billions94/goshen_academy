{
  "name": "oop_express",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "LOG_LEVEL=error NODE_ENV=test jest --config jest.config.js jest && exit 1",
    "build": "rm -rf dist && tsc",
    "start": "node dist/server.js",
    "start:dev": "ts-node-dev ./src/server.ts",
    "start:docker": "npm run build && npm start",
    "sync": "typeorm -- schema:sync",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ormconfig.ts",
    "flag": "--experimental-top-level-await"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.5",
    "@types/bull": "^3.15.9",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.192",
    "@types/multer": "^1.4.11",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "bull": "^4.16.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-api-cache": "^1.0.4",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "pg": "^8.12.0",
    "pino": "^8.11.0",
    "pino-http": "^8.6.1",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.7",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "socket.io": "^4.6.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.13"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testTimeout": 10000,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "endOfLine": "lf",
    "printwidth": 70,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "preserve"
  }
}
